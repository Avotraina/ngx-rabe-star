{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-rabe-star": {
      "projectType": "library",
      "root": "projects/ngx-rabe-star",
      "sourceRoot": "projects/ngx-rabe-star/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-rabe-star/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-rabe-star/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-rabe-star/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-rabe-star/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "2f517fb6-5020-4db1-89b6-d579b3ee557f"
  }
}
